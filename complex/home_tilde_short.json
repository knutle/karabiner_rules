{
  "description": "Double tap left-shift to return ~/",
  "manipulators": [
    {
      "conditions": [
        {
          "name": "left_shift pressed",
          "type": "variable_if",
          "value": 1
        }
      ],
      "from": {
        "key_code": "left_shift",
        "modifiers": { "optional": ["any"] }
      },
      "to": [
        {
          "key_code": "close_bracket",
          "modifiers": ["option"]
        },
        {
          "key_code": "7",
          "modifiers": ["left_shift"]
        }
      ],
      "type": "basic"
    },
    {
      "from": {
        "key_code": "left_shift",
        "modifiers": { "optional": ["any"] }
      },
      "to": [
        {
          "set_variable": {
            "name": "left_shift pressed",
            "value": 1
          }
        },
        { "key_code": "left_shift" }
      ],
      "to_delayed_action": {
        "to_if_canceled": [
          {
            "set_variable": {
              "name": "left_shift pressed",
              "value": 0
            }
          }
        ],
        "to_if_invoked": [
          {
            "set_variable": {
              "name": "left_shift pressed",
              "value": 0
            }
          }
        ]
      },
      "type": "basic"
    }
  ]
}
